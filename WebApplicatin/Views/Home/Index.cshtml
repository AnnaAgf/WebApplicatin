@model IEnumerable<WebApplicatin.Models.SkiResortView>
@{
    ViewData["Title"] = "Index";
}

<h1>SkiResorts</h1>
<table>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Country</td>
            <td>
                    @Html.ActionLink("More", "Details", new { id = item.Id })
            </td>
        </tr>
    }
</table>
