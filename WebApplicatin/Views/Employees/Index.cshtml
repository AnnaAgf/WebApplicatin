@model IEnumerable<EmployeeView>
@{
    ViewData["Title"] = "Список сотрудников";
}

    <div class="container">
        <h1>Employees</h1>
        <table>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.FirstName</td>
                    <td>@item.SurName</td>
                    <td>@item.Age</td>
                    <td>@item.Skill</td>
                    <td>@item.Height</td>
                    <td>@item.Country</td>
                    <td>
                        @Html.ActionLink("More", "Details", new { id = item.Id })
                    </td>
                </tr>
            }
        </table>
    </div>